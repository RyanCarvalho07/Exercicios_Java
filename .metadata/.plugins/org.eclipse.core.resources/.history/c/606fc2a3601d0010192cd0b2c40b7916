		this.carrinho = carrinho;
	}
	
	public boolean adicionarProduto(String nome, Double preco, Integer quantidade ) {
		if (nome == null || nome.isBlank()) {
		    System.out.println("Erro: O nome do produto não pose estár vazio.");
		    return false;
		    
			
		}
		
	}

}
