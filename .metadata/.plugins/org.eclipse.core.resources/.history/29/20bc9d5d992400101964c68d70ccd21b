package entities;

import entities_enum.Color;

public class Circle  extends Shape{
         
	   private Double radius;
	   
	   public Circle() {
		   super();
	   }

	public Circle(Color color, Double radius) {
		super(color);
		this.radius = radius;
	}

	public Double getRadius() {
		return radius;
	}

	public void setRadius(Double radius) {
		this.radius = radius;
	}
	
	
	
	   
	   
}
